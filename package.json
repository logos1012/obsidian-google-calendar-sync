{
  "name": "obsidian-google-calendar-sync",
  "version": "1.0.3",
  "description": "Sync Google Calendar with Obsidian daily notes",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "google-calendar",
    "sync"
  ],
  "author": "jake",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "obsidian": "latest",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.400.0",
    "googleapis": "^126.0.0"
  }
}
